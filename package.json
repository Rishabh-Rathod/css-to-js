{
  "name": "css-to-js",
  "displayName": "Css to CssInJs ",
  "description": "Helps to convert normal css to CssInJs.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.48.0"
  },
  "publisher": "rishabh-rathod",
  "author": "Rishabh Rathod",
  "homepage": "",
  "categories": [
    "Other",
    "Formatters",
    "Snippets"
  ],
  "keywords": [
    "css",
    "css-in-js",
    "css-to-js",
    "keybindings",
    "javascript",
    "JSS",
    "styled components",
    "style conversion"
  ],
  "activationEvents": [
    "onCommand:extension.convertCSStoJS",
    "onLanguage:javascript"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.convertCSStoJS",
        "title": "Convert css-to-js"
      }
    ],
    "configurationDefaults": {
      "[javascript]": {
        "editor.quickSuggestions": {
          "other": true,
          "comments": true,
          "strings": true
        }
      }
    },
    "keybindings": [
      {
        "command": "extension.convertCSStoJS",
        "key": "ctrl+shift+k",
        "mac": "cmd+shift+k",
        "when": "editorTextFocus"
      }
    ]
  },
  "capabilities": {
    "completionProvider": {
      "resolveProvider": "true",
      "triggerCharacters": [
        "."
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.48.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "eslint": "^7.6.0",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "postcss": "^7.0.32",
    "postcss-js": "git+https://github.com/Rishabh-Rathod/postcss-js.git"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Rishabh-Rathod/css-to-js.git"
  }
}
